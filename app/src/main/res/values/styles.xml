<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.


    -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light">
        <!--
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>

        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">

        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>



    <!--视频组件添加的style-->
    <style name="Universal.Widget.ProgressBar" parent="@android:style/Widget.ProgressBar">
        <item name="android:indeterminateDrawable">@drawable/uvv_progress_rotate</item>
        <item name="android:indeterminateDuration">1000</item>
    </style>

    <style name="st_pro_horizontal_goddessvideo" parent="@android:style/Widget.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/goddessvideo_progressbar</item>
    </style>
    <style name="meetwait_progressbar" parent="@android:style/Widget.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/meetwait_progressbar</item>
    </style>

    <style name="my_translucent" parent="android:style/Theme.Translucent.NoTitleBar">
        <item name="android:windowBackground">@color/color_shadow</item>
    </style>



</resources>